/* pages/login/login.wxss */



.form_box {
  width: 100%;
  display: flex;
  margin-top: 70rpx;
  flex-direction: column;
  align-items: center;
}


.input_box {
  width: 630rpx;
  margin: 20rpx auto 0 auto;
  display: flex;
  flex-direction: column;
}

.tips {
  font-size: 24rpx;
  color: #1A1A1A;
  font-weight: 300;
  font-family: Microsoft YaHei;
}

.input_view {
  width: 100%;
  padding: 20rpx 0;
  display: flex;
  flex-direction: row;
  align-items: center;
  border-bottom: 2rpx solid #EEEEEE;
}

.t1 {
  font-size: 32rpx;
  font-family: Microsoft YaHei;
  font-weight: 400;
  color: #1A1A1A;
}

.input {
  margin: 0 30rpx;
  font-size: 32rpx;
  font-family: Microsoft YaHei;
  font-weight: 400;
  color: #1A1A1A;
}

.input2 {
  font-size: 32rpx;
  font-family: Microsoft YaHei;
  font-weight: 400;
  color: #1A1A1A;
}

.submit_btn {
  margin: 70rpx auto 0 auto;
}

.submit_btn:active {
  opacity: 0.7;
}


.t2 {
  line-height: 100rpx;
  font-size: 26rpx;
  font-family: Microsoft YaHei;
  font-weight: 300;
  color: #191919;
}

.bottom_menu {
  width: 600rpx;
  display: flex;
  margin: 0 auto;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}

.bottom_box {
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}

.checkbox {
  width: 36rpx;
  height: 36rpx;
}

.t3 {
  font-size: 27rpx;
  font-family: Microsoft YaHei;
  font-weight: 400;
  margin-left: 10rpx;
  line-height: 70rpx;
  color: #AAAAAA;
}
.t4{
color: #3F7CEA;
}

.top{
  width: 630rpx;
  margin: 120rpx auto 0rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}


.logo {
  width: 45rpx;
  height: 45rpx;
  margin-right: 10rpx;
}

.company-title{
  font-size: 30rpx;
  color: #333;
}

.company-text{
  width: 600rpx;
  margin: 40rpx auto;
  font-size: 26rpx;
  color: #9E9E9E;
  text-align: center;
}

.avatar-wrapper{
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  margin: 0 auto!important;
  padding: 0!important;
  border-radius: 100%;
  background: #fff!important;
}

.set-width{
  width: 103px!important;
  height: 103px!important;
  background: #ededed!important;
}

.small-text{
  font-size: 24rpx;
  color: #000000;
  margin-top: 8rpx;
  font-weight: normal;
}


.avatar {
  width: 142rpx;
  height: 142rpx;
  /* background-color: #e7eaed; */
  background-image: url("data:image/png;base64,");
  background-repeat: no-repeat;
  background-size: cover;
  border-radius: 100%;
}