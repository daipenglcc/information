.header {
  width: 100%;
  height: 339rpx;
  /* background-color: #3f59fb; */
  background-image: url('data:image/png;base64,');
  background-repeat: no-repeat;
  background-size: cover;
  /* background-color: #326ce1; */
  background-position: -68px center;
  display: flex;
  align-items: center;
  justify-content: center;
  /* border-bottom-left-radius: 28px;
  border-bottom-right-radius: 28px; */
  margin-bottom: 40rpx;
}

.inner-container {
  width: calc(100% - 50rpx);
  /* margin: 0 50rpx; */
  /* height: 100px; */
  background-color: #fff;
  display: flex;
  align-items: center;
  justify-content: center;
  position: absolute;
  top: 190rpx;
  border-radius: 30rpx;
  box-shadow: 0 2px 6px rgba(0, 0, 0, 0.1);
  /* background-color: red; */
}

.left-section {
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: flex-start;
  margin-left: 50rpx;
  margin: 33rpx 33rpx;
  /* box-sizing: border-box; */
}

.right-section{
  margin-right: 50rpx;
}

.avatar {
  width: 102rpx;
  height: 102rpx;
  /* background-color: #e7eaed; */
  background-image: url("data:image/png;base64,");
  background-repeat: no-repeat;
  background-size: cover;
  border-radius: 100%;
}

.login-text {
  font-size: 36rpx;
}

.right-section {
  /*  */
}

.list-item {
  padding: 49rpx 25rpx;
  background-color: #fff;
  /* margin-top: 10px; */
  font-size: 30rpx;
  color: #333;
  border-bottom: 1rpx solid #ebebeb;
  display: flex;
    justify-content: space-between;
}

.userinfo{
  display: flex;
  flex-direction: column;
  margin-left: 10px;
}

.login-id{
  font-size: 18rpx;
  margin-top: 10rpx;
  color: #333;
}

.main-text{
  display: flex;
  justify-content: center;
  align-items: center;
}


